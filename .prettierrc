{
  "printWidth": 120,
  "tabWidth": 2,
  "endOfLine": "lf",

  "singleQuote": true,
  "jsxSingleQuote": false,
  "semi": false,

  "trailingComma": "all",
  "bracketSpacing": true,
  "bracketSameLine": true,
  "arrowParens": "avoid",

  "htmlWhitespaceSensitivity": "css",
  "vueIndentScriptAndStyle": false,
  "singleAttributePerLine": false,

  "objectWrap": "preserve",
  "proseWrap": "always"
}
